{
	"name": "bingo",
	"version": "0.0.1",
	"packageManager": "pnpm@7.14.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"package": "vite package",
		"preview": "vite preview",
		"create-types": "supabase gen types typescript --local > src/lib/db/types.ts",
		"create-types-check": "if scripts/if-newer-than.sh supabase/migrations src/lib/db/types.ts; then npm run create-types; fi",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"prepare": "husky install;"
	},
	"devDependencies": {
		"@supabase/supabase-js": "^2.0.5",
		"@sveltejs/adapter-auto": "^1.0.0-next.80",
		"@sveltejs/kit": "^1.0.0-next.528",
		"@tailwindcss/typography": "^0.5.1",
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"@typescript-eslint/parser": "^5.10.1",
		"autoprefixer": "^10.4.2",
		"cookie-es": "^0.5.0",
		"cssnano": "^5.0.17",
		"daisyui": "^1.25.4",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"husky": "^7.0.4",
		"openapi-typescript": "^5.1.1",
		"postcss": "^8.4.5",
		"postcss-load-config": "^3.1.1",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"pretty-quick": "^3.1.3",
		"supabase-ui-svelte": "^0.1.1",
		"svelte": "^3.44.0",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.10.1",
		"tailwindcss": "^3.0.12",
		"tslib": "^2.3.1",
		"typescript": "^4.4.3",
		"vite": "^3.1.6"
	},
	"type": "module",
	"dependencies": {
		"@supabase/auth-helpers-sveltekit": "^0.8.3",
		"readline": "^1.3.0",
		"svelte-feather-icons": "^4.0.0"
	}
}
