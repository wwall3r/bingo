{
	"name": "bingo",
	"version": "0.0.1",
	"packageManager": "pnpm@7.14.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"package": "vite package",
		"preview": "vite preview",
		"create-types": "supabase gen types typescript --local > src/lib/db/types.ts",
		"create-types-check": "if scripts/if-newer-than.sh supabase/migrations src/lib/db/types.ts; then npm run create-types; fi",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --write --plugin-search-dir=. .",
		"prepare": "husky install;"
	},
	"devDependencies": {
		"@supabase/auth-helpers-sveltekit": "^0.8.6",
		"@supabase/supabase-js": "^2.2.2",
		"@sveltejs/adapter-auto": "1.0.0",
		"@sveltejs/kit": "1.0.1",
		"@tailwindcss/typography": "^0.5.1",
		"@typescript-eslint/eslint-plugin": "^5.46.1",
		"@typescript-eslint/parser": "^5.46.1",
		"autoprefixer": "^10.4.2",
		"cookie-es": "^0.5.0",
		"cssnano": "^5.0.17",
		"daisyui": "^1.25.4",
		"eslint": "^8.29.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"husky": "^7.0.4",
		"openapi-typescript": "^5.1.1",
		"postcss": "^8.4.5",
		"postcss-load-config": "^3.1.1",
		"prettier": "^2.8.1",
		"prettier-plugin-svelte": "^2.9.0",
		"pretty-quick": "^3.1.3",
		"supabase": "^1.27.7",
		"svelte": "^3.55.0",
		"svelte-check": "^3.0.1",
		"svelte-feather-icons": "^4.0.0",
		"tailwindcss": "^3.0.12",
		"tslib": "^2.3.1",
		"typescript": "^4.9.4",
		"vite": "^4.0.1"
	},
	"type": "module"
}
