{
	"name": "bingo",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"create-types": "openapi-typescript \"$(grep URL .env | cut -d '=' -f2)/rest/v1/?apikey=$(grep ANON .env | cut -d '=' -f2)\" --output src/lib/db/types.ts",
		"create-types-check": "if scripts/if-newer-than.sh supabase/migrations src/lib/db/types.ts; then npm run create-types; fi",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"prepare": "husky install;"
	},
	"devDependencies": {
		"@supabase/supabase-js": "^1.29.4",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"@typescript-eslint/parser": "^5.10.1",
		"cookie-es": "^0.5.0",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"husky": "^7.0.4",
		"openapi-typescript": "^5.1.1",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"pretty-quick": "^3.1.3",
		"supabase-ui-svelte": "^0.1.1",
		"svelte": "^3.44.0",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.9.4",
		"tslib": "^2.3.1",
		"typescript": "^4.4.3"
	},
	"type": "module",
	"dependencies": {
		"@supabase/supabase-js": "^1.29.4"
	}
}
